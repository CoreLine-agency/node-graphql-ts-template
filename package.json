{
  "name": "@coreline/graphql-ts-helper",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "lint": "eslint src --ext ts,tsx",
    "start": "ts-node scripts/set-development-env && npm run db:start && sleep 3 && npm run db:migration:up && nodemon",
    "start:production": "node src",
    "proxy": "lt -p 5000",
    "generate": "ts-node src/generator && npm run lint -- --fix",
    "db:migration:create": "ts-node scripts/generate-migration empty",
    "db:migration:generate": "ts-node scripts/generate-migration",
    "db:migration:up": "typeorm migration:run",
    "db:start": "docker-compose -f docker-compose.yml up -d",
    "db:stop": "docker container stop coreline-template",
    "psql": "psql postgresql://user:1234@localhost:41888/coreline-template"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/config": "0.0.34",
    "@types/express": "^4.11.1",
    "@types/lodash": "^4.14.108",
    "@types/node": "^12.11.6",
    "app-root-path": "^2.2.1",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.1",
    "clean-deep": "^3.0.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "dotenv": "^6.0.0",
    "env-var": "^5.1.0",
    "express": "^4.15.3",
    "express-async-wrapper": "^0.1.0",
    "graphql": "^14.5.8",
    "graphql-list-fields": "^2.0.2",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.13",
    "pg": "^7.4.3",
    "pluralize": "^7.0.0",
    "pretty-error": "^2.1.1",
    "raven": "^2.6.4",
    "readline-promise": "^1.0.4",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^8.4.1",
    "type-graphql": "^0.17.5",
    "typeorm": "^0.2.20",
    "typescript": "^3.6.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "eslint": "^6.5.1",
    "localtunnel": "^2.0.0",
    "make-dir": "^1.3.0",
    "nodemon": "^1.19.4"
  }
